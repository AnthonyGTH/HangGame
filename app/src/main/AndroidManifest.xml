<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dev01dev.hanggame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/playstore"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:roundIcon="@mipmap/playstore"
        android:supportsRtl="true">
        <activity android:screenOrientation="portrait"  android:name="com.dev01dev.hanggame.GameActivity2"></activity>
        <activity android:screenOrientation="portrait"  android:name="com.dev01dev.hanggame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>